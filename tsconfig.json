    "compilerOptions": {
        "outDir": "./dist", // onde sera feita o sey de TS para JS
        "module": "commonjs",
        "target": "es2019", //versao do JS(ES) que sera gerada ao compilar o TS
        "esModuleInterop": true, //gerencia modulos do commonjs
        "allowJs": true,
        "strictNullChecks": true
        
    }
}